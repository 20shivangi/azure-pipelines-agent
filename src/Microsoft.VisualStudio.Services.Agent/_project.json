{
    "version": "1.0.0-*",
    "compilationOptions": {
        "emitEntryPoint": false,
        "warningsAsErrors": true
    },

    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE",
                    "OS_WINDOWS"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "TRACE",
                    "OS_WINDOWS"
                ],
                "optimize": true
            }
        }
    },
    
    "dependencies": {
        "vss-api-netcore": "0.5.4-private",
        "Newtonsoft.Json": "7.0.1",
        "System.Diagnostics.TraceSource": "4.0.0-rc3-23829",
        "System.Diagnostics.TextWriterTraceListener": "4.0.0-rc3-23829",
        "System.Diagnostics.Process": "4.1.0-rc3-23829",
        "System.IO": "4.1.0-rc3-23829",
        "System.IO.Pipes": "4.0.0-rc3-23829",
        "System.Linq.Parallel": "4.0.0",
        "System.Runtime": "4.1.0-rc3-23829",
        "System.Xml.XmlSerializer": "4.0.11-rc3-23829",
        "NETStandard.Library": "1.0.0-rc3-23829"
    },

    "frameworks": {
        "dnxcore50": {
            "imports": "portable-net451+win8"
        }
    }
}
