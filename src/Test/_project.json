{
    "version": "1.0.0-*",   
    "compilationOptions": {
        "emitEntryPoint": true,
        "define": [ "SAMPLE_DEFINE" ]
    },
    
    "configurations": {
        "Debug": {
            "compilationOptions": {
                "define": [
                    "DEBUG",
                    "TRACE",
                    "OS_WINDOWS"
                ]
            }
        },
        "Release": {
            "compilationOptions": {
                "define": [
                    "TRACE",
                    "OS_WINDOWS"
                ],
                "optimize": true
            }
        }
    },
    
    "dependencies": {
        "NETStandard.Library": "1.0.0-rc3-23721",
        "Microsoft.NETCore.TestHost": "1.0.0-rc3-*",
        "moq.netcore": "4.4.0-beta8",
        "xunit": "2.1.0",
        "xunit.console.netcore": "1.0.2-prerelease-00101",
        "xunit.netcore.extensions": "1.0.0-prerelease-00153",
        "xunit.runner.utility": "2.1.0",
        "Agent.Listener": { "target": "project" },
        "Microsoft.TeamFoundation.Common": "0.5.0",
        "Microsoft.TeamFoundation.Core.WebApi": "0.5.0",
        "Microsoft.VisualStudio.Services.Client": "0.5.0",
        "Microsoft.VisualStudio.Services.Common": "0.5.0",
        "Microsoft.VisualStudio.Services.WebApi": "0.5.0",        
        "Microsoft.VisualStudio.Services.Agent": { "target": "project" },
        "Agent.Worker": { "target": "project" } 
    },
    "frameworks": {
        "dnxcore50": {
            "imports": "portable-net451+win8"
        }
    }
}
